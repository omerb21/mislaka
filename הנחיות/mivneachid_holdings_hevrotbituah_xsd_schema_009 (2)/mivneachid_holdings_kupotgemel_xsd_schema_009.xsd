                                                            <xsd:documentation>לפי חוזר קידוד אחיד</xsd:documentation>
                                                          </xsd:annotation>
                                                          <xsd:simpleType>
                                                            <xsd:restriction base="xsd:string">
                                                              <xsd:pattern value="[0-9]{30}"/>
