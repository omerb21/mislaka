                                                              <xsd:element name="GOVA-DMEI-NIHUL-NIKBA-AL-PI-HOTZAOT-BAPOAL" minOccurs="1">
                                                                <xsd:annotation>
                                                                  <xsd:documentation>אחד מן הערכים הבאים: 1-כן 2-לא</xsd:documentation>
                                                                </xsd:annotation>
