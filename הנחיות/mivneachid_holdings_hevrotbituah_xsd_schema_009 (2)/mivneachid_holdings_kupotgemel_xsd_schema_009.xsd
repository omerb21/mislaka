                                                            <xsd:documentation>1 ראשי 2 משני 3 ילד 4 הדדי </xsd:documentation>
                                                          </xsd:annotation>
                                                          <xsd:simpleType>
                                                            <xsd:restriction base="xsd:int">
                                                              <xsd:enumeration value="1"/>
                                                              <xsd:enumeration value="2"/>
                                                              <xsd:enumeration value="3"/>
                                                              <xsd:enumeration value="4"/>
                                                            </xsd:restriction>
                                                          </xsd:simpleType>
                                                        </xsd:element>
