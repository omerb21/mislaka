                                                            <xsd:restriction base="xsd:decimal">
                                                              <xsd:totalDigits value="15"/>
                                                              <xsd:fractionDigits value="2"/>
                                                            </xsd:restriction>
                                                          </xsd:simpleType>
                                                        </xsd:element>
                                                         <xsd:element name="SCHUM-BITUAH-LEMAVET" minOccurs="1" nillable="true">
