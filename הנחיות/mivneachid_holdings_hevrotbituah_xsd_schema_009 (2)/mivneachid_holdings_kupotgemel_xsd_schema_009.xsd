                                                              <xsd:totalDigits value="5"/>
                                                              <xsd:fractionDigits value="2"/>
                                                              <xsd:minInclusive value="0"/>
                                                              <xsd:maxInclusive value="100"/>
                                                            </xsd:restriction>
                                                          </xsd:simpleType>
                                                        </xsd:element>
                                                        <xsd:element name="TIKRAT-GAG-HATAM-LEMIKRE-MAVET" minOccurs="1" nillable="true">
