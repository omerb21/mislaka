                                                                <xsd:annotation>
                                                                  <xsd:documentation>
                                                                    0 = לא רלוונטי 1 = דמי ניהול מצבירה 2 = דמי ניהול מהפקדה
                                                                  </xsd:documentation>
                                                                </xsd:annotation>
                                                                <xsd:simpleType>
                                                                  <xsd:restriction base="xsd:int">
                                                                    <xsd:enumeration value="0"/>
                                                                    <xsd:enumeration value="1"/>
                                                                    <xsd:enumeration value="2"/>
                                                                  </xsd:restriction>
                                                                </xsd:simpleType>
                                                              </xsd:element>
                                                              <xsd:element name="KOD-MASLUL-DMEI-NIHUL" minOccurs="1" nillable="true">
