                                                            </xsd:documentation>
                                                          </xsd:annotation>
                                                          <xsd:simpleType>
                                                            <xsd:restriction base="xsd:int">
                                                              <xsd:enumeration value="1"/>
                                                              <xsd:enumeration value="2"/>
                                                            </xsd:restriction>
                                                          </xsd:simpleType>
                                                        </xsd:element>
                                                        <xsd:element name="SCHUM-BITUACH-LEMASLUL" minOccurs="1" nillable="true">
